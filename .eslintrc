{
  "plugins": ["prettier"],

  "extends": [],

  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": false
    }
  },

  "env": {
    "browser": false,
    "node": true
  },
  "ignorePatterns": ["node_modules/"],

  "rules": {
    "prettier/prettier": ["error", { "endOfLine": "auto" }]
  }
}
